<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:id="@+id/activity_address_map"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clipToPadding="true"
                android:fitsSystemWindows="true"
                tools:context="cn.com.hiss.www.multilib.ui.AddressMapActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <cn.com.hiss.www.multilib.hissviews.HissTitleBar
            android:id="@+id/id_hiss_title_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></cn.com.hiss.www.multilib.hissviews.HissTitleBar>

        <com.amap.api.maps.MapView
            android:id="@+id/map"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </LinearLayout>

</RelativeLayout>
